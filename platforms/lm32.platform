GCC_VER="4.5.3"
NEWLIB_VER="1.19.0"
LANGUAGES="c,c++"
GCC_EXTRA_CONFIG="--enable-sjlj-exceptions"
GDB_VER="7.3.1"
BUILD_GDB_SERVER=0
